#### 	Openbox autostart file
####
####	~/.config/openbox/autostart


# setup some useful variables for this script
	SCRIPTS_DIR = /scripts ;


# Desktop & DE
	/scripts/pcmanfm_toggle_desktop_icons.sh setup &
	tint2 -c ~/.config/tint2/e6400-horizontal.tint2rc &
	nm-applet &
	tilda &
	imwheel &
	lxsession & 
	sleep 2s; opensnap &
	


